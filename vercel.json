{
  "framework": null,
  "buildCommand": "npm install && npx nx build fortnite-radar",
  "outputDirectory": "dist/apps/fortnite-radar/browser",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/apps/fortnite-radar/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
